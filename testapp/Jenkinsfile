Pipeline {
	agent any
	Stages {
		stage('build') {
			steps {
				sh   'build the job' 
				}
			}
		stage('script') {
			steps {
				sh   'running due to manual ' 
				}
			}
		stage('script') {
		when {
            triggeredBy 'TimerTrigger'
			steps {
				sh   'running through the timertrigger' 
				}
			}
        }
    } 
}          		
